    <%- include('../Partials/header.ejs') %>
    <div class="content">
        <div id="character-container" class="row">
            <% for(var i = 0; i < data.length; i++) { %>
                <div class="col-md-3 mb-3 align-items-stretch">
                    <div class="card h-100">
                        <img src="<%- data[i].imageUrl %>" class="card-img-top" alt="Character Image">
                        <div class="card-body">
                            <h5 class="card-title"><%- data[i].id %></h5>
                            <p class="card-text name"><%- data[i].fullName %></p>
                            <p class="card-text"><h7>First Name: </h7><%- data[i].firstName %></p>
                            <p class="card-text"><h7>Last Name: </h7><%- data[i].lastName %></p>
                            <p class="card-text"><h7>Title: </h7><%- data[i].title %></p>
                            <p class="card-text"><h7>Family: </h7><%- data[i].family %></p>
                            
                        </div>
                    </div>
                </div>

            <% } %>
        </div>

        <!-- Navigation Buttons -->
        <!-- <div class="pagination-container">
            <button id="prev-button" class="btn btn-secondary">Previous</button>
            
            <button id="next-button" class="btn btn-secondary">Next</button>
        </div> -->
    </div>
        <%- include('../Partials/footer.ejs') %>